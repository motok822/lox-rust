// テスト1: forループで1から10まで
print "Counting 1 to 10:";
for (var i = 1; i <= 10; i = i + 1) {
    print i;
}

// テスト2: whileループ
print "Countdown:";
var count = 5;
while (count > 0) {
    print count;
    count = count - 1;
}
print "Blast off!";

// テスト3: break文
print "Find first number > 7:";
for (var i = 0; i < 20; i = i + 1) {
    if (i > 7) {
        print i;  // 8が出力される
        break;
    }
}

// テスト4: continue文
print "Even numbers from 0 to 10:";
for (var i = 0; i <= 10; i = i + 1) {
    if (i % 2 == 1) {
        continue;
    }
    print i;  // 0, 2, 4, 6, 8, 10
}
